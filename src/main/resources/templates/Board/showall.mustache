{{>layouts/header}}

<div class="mt-2 mb-3 ml-3">
    <form action="/board">
        <button type="submit" class="btn btn-primary">글쓰기</button>
    </form>
</div>


<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#resultkey}}

        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{contents}}</td>
        </tr>

    {{/resultkey}}
    </tbody>
</table>



{{>layouts/footer}}