{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#boardkey}}

    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{contents}}</td>
    </tr>

    {{/boardkey}}


    </tbody>
</table>

<div class="mb-3 mr-5" align="right">
    <form action="http://localhost:8080/board/{{boardkey.id}}/edit/">
        <button type="submit" class="btn btn-warning">
            <h4>
                Edit
            </h4>
        </button>
    </form>
</div>

<div class="mb-3 mr-5" align="right">
    <form action="http://localhost:8080/board/{{boardkey.id}}/delete/">
        <button type="submit" class="btn btn-danger">
            <h4>
                Delete
            </h4>
        </button>
    </form>
</div>


<div class="mb-3 ml-3">
    <form action="/board/all/">
        <button type="submit" class="btn btn-primary">
            <h4>
                Board
            </h4>
        </button>
    </form>
</div>

<div class="mt-5 ml-3">
    <form action="/">
        <button type="submit" class="btn btn-success">
            <h4>
                Home
            </h4>
        </button>
    </form>
</div>




{{>layouts/footer}}