{{>layouts/header}}
<div class="mt-5 mb-5" align="center">
    <h1>
        <a href="http://localhost:8080/board/all/"> 게시판 보기 </a>
    </h1>
</div>

{{#editkey}}
<form class="container" action="http://localhost:8080/board/" method="post" >
    <div class="mb-3 mt-3">
        <label class="form-label">제목</label>
        <input type="text" class="form-control" name="title" value="{{title}}">
    </div>
    <div class="mb-3">
        <label class="form-label">내용</label>
        <textarea class="form-control" rows="5" name="contents">{{contents}}</textarea>
    </div>
    <button type="submit" class="btn btn-primary">수정</button>
</form>
{{/editkey}}


{{>layouts/footer}}
